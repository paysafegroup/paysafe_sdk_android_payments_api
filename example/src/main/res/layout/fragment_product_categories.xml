<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/clProdCategoriesRoot"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.paysafe.example.ActivitySample">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerProductCategoriesList"
        android:layout_width="@dimen/no_size"
        android:layout_height="@dimen/no_size"
        android:clipToPadding="false"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:itemCount="4"
        tools:listitem="@layout/rv_product_category_item" />

</androidx.constraintlayout.widget.ConstraintLayout>